<h1>My Accounts</h1>
<script>
    const swiper = new Swiper('.swiper', {
        // Optional parameters
        direction: 'vertical',
        loop: true,

        // If we need pagination
        pagination: {
            el: '.swiper-pagination',
        },

        // Navigation arrows
        navigation: {
            nextEl: '.swiper-button-next',
            prevEl: '.swiper-button-prev',
        },

        // And if we need scrollbar
        scrollbar: {
            el: '.swiper-scrollbar',
        },
    });
</script>
<style>
    .swiper {
        width: 600px;
        height: 300px;
    }
</style>

<table>
    <thead>
        <tr>
            <th>First name</th>
            <th>Last name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Address</th>
            <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @my_accounts.each do |my_account| %>
        <tr>
            <td><%= my_account.first_name %></td>
            <td><%= my_account.last_name %></td>
            <td><%= my_account.email %></td>
            <td><%= my_account.phone %></td>
            <td><%= my_account.address %></td>
            <td><%= link_to 'Show', my_account %></td>
            <td><%= link_to 'Edit', edit_my_account_path(my_account) %></td>
            <td><%= link_to 'Destroy', my_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
        <% end %>
    </tbody>
</table>

